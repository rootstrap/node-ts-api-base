{
  "name": "express-api-base",
  "version": "1.0.0",
  "description": "Rest api base with nodejs + express",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/server.ts",
    "prebuild": "rm -rf /dist",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register -r ts-node/register ./dist/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "ENVIRONMENT=test jest",
    "test:ci": "ENVIRONMENT=ci jest",
    "lint": "eslint \"src/**\" --color",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/jest": "^26.0.12",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.6.2",
    "@types/supertest": "^2.0.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@admin-bro/express": "^3.0.0",
    "@admin-bro/typeorm": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "admin-bro": "^3.2.3",
    "admin-bro-expressjs": "^2.1.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "jest": "^26.4.2",
    "multer": "^1.4.2",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25"
  }
}
